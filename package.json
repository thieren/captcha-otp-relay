{
  "name": "captcha-otp-relay",
  "version": "0.1.0",
  "description": "relay your captcha and otp requests in console applications to an accessible format",
  "main": "dist/index.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build-server && npm run build-frontend",
    "build-frontend": "ng build",
    "build-server": "rimraf ./dist && tsc",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thieren/captcha-otp-relay"
  },
  "keywords": [
    "captcha",
    "otp"
  ],
  "author": "thieren",
  "license": "MIT",
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.4",
    "@angular/animations": "^15.0.0",
    "@angular/cli": "^15.0.4",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/compiler-cli": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "@ng-bootstrap/ng-bootstrap": "^14.0.0",
    "@types/express": "^4.17.15",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^18.11.17",
    "bootstrap": "^5.2.3",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "rimraf": "^3.0.2",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "typescript": "~4.8.2",
    "zone.js": "~0.12.0"
  },
  "dependencies": {
    "body-parser": "^1.20.1",
    "express": "^4.18.2",
    "tiny-typed-emitter": "^2.1.0",
    "ts-log": "^2.2.5"
  }
}
